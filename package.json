{
  "name": "@cmtlyt/logger",
  "version": "0.3.1",
  "type": "module",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "author": {
    "name": "cmtlyt",
    "email": "cmtlyt@163.com"
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "scripts": {
    "build": "rslib build",
    "check": "biome check --write",
    "dev": "rslib build --watch",
    "format": "biome format --write",
    "prepublishOnly": "pnpm run build"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.1",
    "@cmtlyt/logger": "^0.2.8",
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@rslib/core": "^0.18.4",
    "@types/node": "^24.10.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "typescript": "^5.9.3"
  },
  "devEngines": {
    "runtime": {
      "name": "node",
      "version": ">=18.0.0"
    },
    "packageManager": {
      "name": "pnpm"
    }
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    },
    "./adapters": {
      "import": {
        "types": "./dist/adapters/index.d.ts",
        "default": "./dist/adapters/index.js"
      },
      "require": {
        "types": "./dist/adapters/index.d.ts",
        "default": "./dist/adapters/index.cjs"
      }
    },
    "./adapters/web": {
      "import": {
        "types": "./dist/adapters/web/index.d.ts",
        "default": "./dist/adapters/web/index.js"
      },
      "require": {
        "types": "./dist/adapters/web/index.d.ts",
        "default": "./dist/adapters/web/index.cjs"
      }
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "homepage": "https://github.com/cmtlyt/logger/blob/main/README.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cmtlyt/logger.git"
  },
  "bugs": {
    "url": "https://github.com/cmtlyt/logger/issues",
    "email": "cmtlyt@163.com"
  },
  "keywords": [
    "cmtlyt",
    "logger",
    "日志输出",
    "埋点"
  ]
}
